    "moonsharp",
    "typescript",
    "tstl",
    "lua"
  ],
  "repository": "https://github.com/tts-community/types-moonsharp",
  "license": "MIT",
  "author": "tts-community",
  "files": [
    "types"
  ],
  "types": "types",
